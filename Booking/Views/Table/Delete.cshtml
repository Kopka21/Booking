@model Booking.Models.Table

@{
    ViewData["Title"] = "Delete";
}

<h1>Delete</h1>

<form asp-controller="Table" asp-action="DeletePost" method="post">
    <input asp-for="Id" hidden />
    <p><input type="text" value="@Model.Id" disabled="disabled" /></p>
    <p><input type="text" value="@Model.NumberOfSeats" disabled="disabled" /></p>

    <p><input class="btn btn-danger" type="submit" value="Usuń" /></p>

</form>
